PDFLATEX = @pdflatex_path@

all: chklref.pdf

chklref.pdf: chklref.tex
	$(PDFLATEX) $<
	$(PDFLATEX) $<


clean:
	\rm -f chklref.dvi chklref.log chklref.aux chklref.out 

distclean:: clean
	\rm -f chklref.pdf
